#include <stdio.h>
#include <stdlib.h>

int main()

{
    int codigo_produto1, codigo_produto2, quantidade1, quantidade2;
    float preco1, preco2, valor_total;

    scanf("%d %d %f", &codigo_produto1, &quantidade1, &preco1);
    scanf("%d %d %f", &codigo_produto2, &quantidade2, &preco2);

    valor_total = quantidade1 * preco1 + quantidade2 * preco2;

    printf("VALOR A PAGAR: R$ %.2f\n", valor_total);

    return 0;
}
