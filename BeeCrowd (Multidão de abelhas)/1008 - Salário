#include <stdio.h>

int main() {
    int numFunc, horasTrabalhadas;
    double valorHora, salario;

    scanf("%d %d %lf", &numFunc, &horasTrabalhadas, &valorHora);

    salario = horasTrabalhadas * valorHora;

    printf("NUMBER = %d\n", numFunc);
    printf("SALARY = U$ %.2lf\n", salario);

    return 0;
}
